{
  "name": "@synstate/docs",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "pnpm run doc:embed && astro build",
    "dev": "astro dev",
    "doc": "run-s doc:embed",
    "doc:embed": "tsx ./scripts/embed-examples.mts",
    "fmt": "format-uncommitted",
    "fmt:full": "pnpm run z:prettier .",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "preview": "astro preview",
    "z:prettier": "prettier --config ../../.prettierrc --ignore-path ../../.prettierignore --ignore-path ../../.gitignore --ignore-unknown --no-error-on-unmatched-pattern --write"
  },
  "dependencies": {
    "@astrojs/starlight": "^0.37.0",
    "astro": "^5.5.0"
  },
  "devDependencies": {
    "@reduxjs/toolkit": "^2.8.2",
    "@types/argparse": "^2.0.17",
    "@types/react": "^19.2.14",
    "argparse": "^2.0.1",
    "jotai": "^2.12.5",
    "mobx": "^6.13.8",
    "react": "^19.2.4",
    "rxjs": "^7.8.2",
    "zustand": "^5.0.5"
  }
}
